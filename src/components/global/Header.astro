<script>
  const menuButton = document.getElementById('mobile-toggle');
  const mobileNavLinks = document.querySelectorAll('.header-nav-links');

  menuButton?.addEventListener('click', (event) => {
    event.stopPropagation();
    const isExpanded = menuButton.getAttribute('aria-expanded') === 'true';
    
    // Toggle aria-expanded on button
    menuButton.setAttribute('aria-expanded', !isExpanded);
    
    // Toggle show-menu class on nav links
    mobileNavLinks.forEach(nav => {
      if (isExpanded) {
        nav.classList.remove('show-menu');
      } else {
        nav.classList.add('show-menu');
      }
    });
  });

  // Close mobile menu when clicking outside
  document.addEventListener('click', (event) => {
    if (!event.target.closest('.header-nav')) {
      menuButton?.setAttribute('aria-expanded', 'false');
      mobileNavLinks.forEach(nav => {
        nav.classList.remove('show-menu');
      });
    }
  });

  // Get all dropdown buttons
  const dropdownButtons = document.querySelectorAll('.dropdown-button');
  
  dropdownButtons.forEach(button => {
    button.addEventListener('click', () => {
      const isExpanded = button.getAttribute('aria-expanded') === 'true';
      const dropdownMenu = button.nextElementSibling;
      
      // Toggle aria-expanded
      button.setAttribute('aria-expanded', !isExpanded);
      // Toggle aria-hidden on menu
      dropdownMenu.setAttribute('aria-hidden', isExpanded);
      
      // Close other open dropdowns
      dropdownButtons.forEach(otherButton => {
        if (otherButton !== button) {
          otherButton.setAttribute('aria-expanded', 'false');
          otherButton.nextElementSibling.setAttribute('aria-hidden', 'true');
        }
      });
    });
  });

  // Close dropdown when clicking outside
  document.addEventListener('click', (event) => {
    if (!event.target.closest('.dropdown')) {
      dropdownButtons.forEach(button => {
        button.setAttribute('aria-expanded', 'false');
        button.nextElementSibling.setAttribute('aria-hidden', 'true');
      });
    }
  });
</script>
